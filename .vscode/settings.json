{
  "bazel.projectview.open": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/CVS": true,
    ".eclipse": false,
    ".git": true,
    "src": false,
    "bazel-bin": true,
    "bazel-testlogs": true,
    "bazel-out": true,
    "bazel-ynab-monthly-report": true,
    ".vscode": true
  },
  "java.configuration.updateBuildConfiguration": "disabled",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
    "**/!(.eclipse|src)/**": true
  }
}